{
  "name": "astro-kit",
  "version": "1.0.0",
  "private": true,
  "workspaces": [
    "frontend",
    "studio"
  ],
  "scripts": {
    "dev": "npm-run-all --parallel dev:*",
    "build": "npm-run-all --parallel build:*",
    "start": "npm-run-all --parallel start:*",
    "dev:frontend": "npm run dev --workspace=frontend",
    "dev:studio": "npm run dev --workspace=studio",
    "build:frontend": "npm run build --workspace=frontend",
    "build:studio": "npm run build --workspace=studio",
    "preview:frontend": "npm run preview --workspace=frontend",
    "start:studio": "npm run start --workspace=studio",
    "deploy:studio": "npm run deploy --workspace=studio"
  },
  "devDependencies": {
    "npm-run-all": "^4.1.5"
  },
  "engines": {
    "node": ">=18.0.0"
  }
}
